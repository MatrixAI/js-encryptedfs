{
  "name": "encryptedfs",
  "version": "2.1.3",
  "author": "Roger Qiu",
  "description": "Encrypted filesystem written in TypeScript for Node.js",
  "keywords": [
    "filesystem",
    "encryption"
  ],
  "contributors": [
    "Aashwin Varshney",
    "Robert Cronin"
  ],
  "license": "Apache-2.0",
  "repository": {
