{
  "name": "encryptedfs",
  "version": "0.1.0",
  "author": "Roger Qiu",
  "description": "Encrypted filesystem written in TypeScript for Node.js",
  "keywords": [
    "filesystem",
    "encryption"
  ],
  "contributors": [
    "Aashwin Varshney",
    "Robert Cronin"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatrixAI/js-encryptedfs.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "develop": "webpack --mode=development --watch & webpack-dev-server --mode=development & wait",
    "build": "tsc; typedoc --out docs src; webpack --mode='production'",
    "lint": "eslint '{src,tests}/**/*.{js,ts}' --quiet --fix",
    "test": "jest",
    "dos2unix": "dos2unix dist/*",
    "chmod": "chmod u+x dist/bin/*"
  },
  "jest": {
    "verbose": true
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "dependencies": {
    "@types/readable-stream": "2.3.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "auto-bind-proxy": "^1.0.1",
    "buffer": "^5.6.0",
    "errno": "^0.1.7",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "readable-stream": "^3.6.0",
    "threads": "1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^25.0.0",
    "@types/node": "^13.13.1",
    "jest": "^25.0.0",
    "threads-plugin": "^1.3.1",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3",
    "virtualfs": "^2.2.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "bugs": {
    "url": "https://github.com/MatrixAI/js-encryptedfs/issues"
  },
  "homepage": "https://github.com/MatrixAI/js-encryptedfs#readme"
}
