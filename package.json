    "benny": "^3.6.15",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.6.2",
    "systeminformation": "^5.8.9",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
